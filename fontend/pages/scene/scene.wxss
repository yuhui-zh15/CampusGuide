/**index.wxss**/
.container {
  height: 1140rpx;
}

.photo {
  flex-direction: row;
  display: flex;
  height: 350rpx;
  width: 100%;
  /* background: lightgray; */
  margin-bottom: 10rpx;
}


.left {
  display: flex;
  width: 15%;
  height: 100%;
  background: #eeeeee;
  color: gray;  
  writing-mode: vertical-lr;
  text-align: center;
  justify-content: center; 
  align-items: center;
}

.right {
  display: flex;
  width: 85%;
  height: 100%;
  /* background: greenyellow; */
}

.right image {
  height: 100%;
  width: 100%;
}
